    "babel-plugin-css-modules-transform": "^1.6.2",
    "chai": "^4.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "24.7.1",
    "jest-environment-enzyme": "^7.0.2",
    "jest-enzyme": "^7.0.2",
    "regenerator-runtime": "^0.13.2"
  },
  "jest": {
    "setupFiles": [
      "./test/jestsetup.js"
