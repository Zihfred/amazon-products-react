{
  "name": "amazon-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bootstrap": "^4.3.1",
    "enzyme-to-json": "^3.3.5",
    "prettier": "^1.18.2",
    "prop-types": "^15.7.2",
    "query-string": "^6.8.1",
